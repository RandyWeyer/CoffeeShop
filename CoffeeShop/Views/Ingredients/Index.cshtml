@{
  Layout = "_Layout";
}
<h1>Our recipe list</h1>

@if (Model.Count != 0)
{
  <ol>
    @foreach (var item in Model["drinks"])
    {
      <li>@item.GetName() Units| <a href="/inventory" class="deleteAction" id="@item.GetId()">Delete</a></li>
    }
    @foreach (var item in Model["inventories"])
    {
      <li>@item.GetItem()</li>
    }

 </ol>
}
